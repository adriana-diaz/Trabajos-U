    private void TxtclientesActionPerformed(java.awt.event.ActionEvent evt) {                                            

    }                                           


    private void ButagregarcliActionPerformed(java.awt.event.ActionEvent evt) {                                              
        DefaultTableModel modelo = (DefaultTableModel) Tablaclientes.getModel();
        // GENERO LOS VECTORES CON LOS VALORES A AGREGAR//
        Object[] fila = new Object[4];
        fila[0] = texto = Txtclientes.getText();
        fila[1] = Txtnombre.getText();
        fila[2] = Txtdireccion.getText();
        fila[3] = Txttelefono.getText();

        // GENERO UN VECTOR CON LOS VALORES A AGREGARN EN UNA COLUMNA//
        modelo.addRow(fila);
        Tablaclientes.setModel(modelo);
        Txtclientes.setText(texto);
        Txtnombre.setText(null);
        Txtdireccion.setText(null);
        Txttelefono.setText(null);
    }                                             

    private void AgregarfacturasActionPerformed(java.awt.event.ActionEvent evt) {                                                
        DefaultTableModel modelo = (DefaultTableModel) Tablafacturas.getModel();
        // GENERO LOS VECTORES CON LOS VALORES A AGREGAR//
        Object[] fila = new Object[3];
        fila[0] = facturas = TxtFactura.getText();
        fila[1] = Txtfecha.getText();
        fila[2] = texto = Txtclientes.getText();

        // GENERO UN VECTOR CON LOS VALORES A AGREGARN EN UNA COLUMNA//
        modelo.addRow(fila);
        Tablafacturas.setModel(modelo);
        Txtfactura.setText(facturas);
        Txtfecha.setText(null);
        Txtclientes.setText(texto);
    }                                               

