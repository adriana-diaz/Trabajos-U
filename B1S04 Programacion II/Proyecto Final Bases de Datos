    }                                           

    private void EliminarcatActionPerformed(java.awt.event.ActionEvent evt) {                                            
         // PROGRAMA UN BOTON PARA LIMPIAR LA TABLA//
        DefaultTableModel modelo = (DefaultTableModel) Tablacate.getModel();
        modelo.setRowCount(0);
        Tablacate.setModel(modelo);
        Txtcategoria.setText(categoria);
        Txtdescat.setText(null);
    }                                           

    private void SalircateActionPerformed(java.awt.event.ActionEvent evt) {                                          
        this.dispose();
    }                                         

    public static void main(String args[]) {

        java.awt.EventQueue.invokeLater(new Runnable() {
            public void run() {
                new Pantallaprin().setVisible(true);
            }
        });
    }

