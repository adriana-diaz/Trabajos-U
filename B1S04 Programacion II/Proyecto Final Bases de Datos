    private void AgregarproductosActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        DefaultTableModel modelo = (DefaultTableModel) Tablaproductos.getModel();
        // GENERO LOS VECTORES CON LOS VALORES A AGREGAR//
        Object[] fila = new Object[5];
        fila[0] = producto = Txtproducto.getText();
        fila[1] = Txtdescri.getText();
        fila[2] = Txtprecio.getText();
        fila[3] = categoria = Txtcategoria.getText();
        fila[4] = Proveedores = Txtproveedores.getText();

        // GENERO UN VECTOR CON LOS VALORES A AGREGARN EN UNA COLUMNA//
        modelo.addRow(fila);
        Tablaproductos.setModel(modelo);
        Txtproducto.setText(producto);
        Txtdescri.setText(null);
        Txtprecio.setText(null);
        Txtcategoria.setText(categoria);
        Txtproveedores.setText(Proveedores);
    }                                                

