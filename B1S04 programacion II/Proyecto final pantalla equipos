    private void ButaceptarActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // PROGRAMA PARA QUE EL BOTON "AGREGAR" AGREDUE LOS VALORES//
        DefaultTableModel modelo = (DefaultTableModel) Tbldatos.getModel();
        // GENERO LOS VECTORES CON LOS VALORES A AGREGAR//
        Object[] fila = new Object[20];
        fila[0] = Txtid.getText();
        fila[1] = Txtnombre.getText();
        fila[2] = Txtnombreof.getText();
        fila[3] = Txtdireccion.getText();
        fila[4] = Txtcp.getText();
        fila[5] = Txtdireinter.getText();
        fila[6] = Cmbprovincia.getSelectedItem().toString();
        // PROGRAMO UN IF PARA EL RADIO BUTTON//
        if (RdCR.isSelected()) {
            fila[7] = "Costa Rica";
        } else if (Rdotro.isSelected()) {
            fila[7] = "Otro";
        } else {
            fila[7] = "No se selecciono";
        }
        fila[8] = Spifecha.getValue();
        fila[9] = Txtpenas.getText();
        fila[10] = Txtemail.getText();
        // PROGRAMO UN IF PARA EL CHECK BOX//
        if (Chbsi.isSelected()) {
            fila[11] = "Si";
        } else if (Chbno.isSelected()) {
            fila[11] = "No";
        } else {
            fila[11] = "No se selecciono";
        }
        fila[12] = Txttelefono.getText();
        fila[13] = Txtfax.getText();
        fila[14] = Txthimno.getText();
        fila[15] = Txthistoria.getText();
        fila[16] = Txtotrassec.getText();
        fila[17] = Txtlocalidad.getText();

        // GENERO UN VECTOR CON LOS VALORES A AGREGARN EN UNA COLUMNA//
        modelo.addRow(fila);
        Tbldatos.setModel(modelo);
        Txtnombre.setText(null);
        Txtnombreof.setText(null);
        Txtdireccion.setText(null);
        Txtcp.setText(null);
        Txtdireinter.setText(null);
        Cmbprovincia.setSelectedItem(null);
        Spifecha.setValue(fila);
        Txtpenas.setText(null);
        Txtemail.setText(null);
        Txttelefono.setText(null);
        Txtfax.setText(null);
        Txthimno.setText(null);
        Txthistoria.setText(null);
        Txtotrassec.setText(null);
        Txtlocalidad.setText(null);
    }                                          

    private void ButLimpiarActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // PROGRAMA UN BOTON PARA LIMPIAR LA TABLA//
        DefaultTableModel modelo = (DefaultTableModel) Tbldatos.getModel();
        modelo.setRowCount(0);
        Txtnombre.setText(null);
        Txtnombreof.setText(null);
        Txtdireccion.setText(null);
        Txtcp.setText(null);
        Txtdireinter.setText(null);
        Cmbprovincia.setSelectedItem(null);
        Spifecha.setValue(null);
        Txtpenas.setText(null);
        Txtemail.setText(null);
        Txttelefono.setText(null);
        Txtfax.setText(null);
        Txthimno.setText(null);
        Txthistoria.setText(null);
        Txtotrassec.setText(null);
        Txtlocalidad.setText(null);

    }                                          

    private void ButSalirActionPerformed(java.awt.event.ActionEvent evt) {                                         
        this.dispose();
    }                                        

    private void ButEliminarActionPerformed(java.awt.event.ActionEvent evt) {                                            
         //  PROGRAMA UN BOTON PARA ELIMINAR LOS VALORES DE LA TABLA
        DefaultTableModel modelo = (DefaultTableModel) Tbldatos.getModel();
        int seleccion = Tbldatos.getSelectedRow();
        if (seleccion < 0)
        {
            //PREGUNTO AL USUARIO QUE Debe seleccionar una fila de la tabla//
            JOptionPane.showMessageDialog(null, "Debe seleccionar una fila de la tabla");
        }
        else{
             //IMPRIMO UN TEXTO AL USUARIO SI Esta seguro de eliminar esta fila//
             int confirmar = JOptionPane.showConfirmDialog(null, "Esta seguro de eliminar esta fila");
             if (JOptionPane.OK_OPTION == confirmar)
             {
                 //IMPRIMO UN TEXTO AL USUARIO QUE EL REGISTRO ESTA ELIMINADO//
             modelo.removeRow(seleccion);
             JOptionPane.showMessageDialog(null, "Registro Eliminado");
             }
             }
    }                                           

