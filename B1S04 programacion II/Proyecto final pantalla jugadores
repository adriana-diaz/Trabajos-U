    private void ButEliminarActionPerformed(java.awt.event.ActionEvent evt) {                                            
         //  PROGRAMA UN BOTON PARA ELIMINAR LOS VALORES DE LA TABLA
        DefaultTableModel modelo = (DefaultTableModel) Tbldatos.getModel();
        int seleccion = Tbldatos.getSelectedRow();
        if (seleccion < 0)
        {
            //PREGUNTO AL USUARIO QUE Debe seleccionar una fila de la tabla//
            JOptionPane.showMessageDialog(null, "Debe seleccionar una fila de la tabla");
        }
        else{
             //IMPRIMO UN TEXTO AL USUARIO SI Esta seguro de eliminar esta fila//
             int confirmar = JOptionPane.showConfirmDialog(null, "Esta seguro de eliminar esta fila");
             if (JOptionPane.OK_OPTION == confirmar)
             {
                 //IMPRIMO UN TEXTO AL USUARIO QUE EL REGISTRO ESTA ELIMINADO//
             modelo.removeRow(seleccion);
             JOptionPane.showMessageDialog(null, "Registro Eliminado");
             }
             }
    }                                           

