    }                                         

    private void ButaceptarActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // PROGRAMA PARA QUE EL BOTON "AGREGAR" AGREDUE LOS VALORES//
        DefaultTableModel modelo = (DefaultTableModel) Tbldatos.getModel();
        // GENERO LOS VECTORES CON LOS VALORES A AGREGAR//
        Object[] fila = new Object[20];
        fila[0] = Txtid.getText();
        fila[1] = Txtnombre.getText();
        fila[2] = Txtnombrefut.getText();
        fila[3] = Txtapellidos.getText();
        fila[4] = Txtfechadenac.getText();
        fila[5] = Txtlugardenac.getText();
        fila[6] = Cmbprovincia.getSelectedItem().toString();
        // PROGRAMO UN IF PARA EL RADIO BUTTON//
        if (RdCR.isSelected()) {
            fila[7] = "Costa Rica";
        } else if (Rdotro.isSelected()) {
            fila[7] = "Otro";
        } else {
            fila[7] = "No se selecciono";
        }
        fila[8] = Spipie.getValue();
        fila[9] = Txtdemarcacion.getText();
        fila[11] = Txtsituacion.getText();
        fila[10] = Txtaltura.getText();
        fila[12] = Txtpeso.getText();
        fila[13] = Txtcantera.getText();
        fila[14] = Txtinternacional.getText();
        fila[15] = Txthistorial.getText();
        fila[16] = Txtrepresentante.getText();
        fila[17] = Txtpaisnacionalidad.getText();
        // PROGRAMO UN IF PARA EL CHECK BOX//
        if (Chbsi.isSelected()) {
            fila[18] = "Si";
        } else if (Chbno.isSelected()) {
            fila[18] = "No";
        } else {
            fila[18] = "No se selecciono";
        }

        //
        // GENERO UN VECTOR CON LOS VALORES A AGREGARN EN UNA COLUMNA//
        modelo.addRow(fila);
        Tbldatos.setModel(modelo);
        Txtnombre.setText(null);
        Txtnombrefut.setText(null);
        Txtapellidos.setText(null);
        Txtfechadenac.setText(null);
        Txtlugardenac.setText(null);
        Cmbprovincia.setSelectedItem(null);
        Spipie.setValue(fila);
        Txtdemarcacion.setText(null);
        Txtsituacion.setText(null);
        Txtaltura.setText(null);
        Txtpeso.setText(null);
        Txtcantera.setText(null);
        Txtinternacional.setText(null);
        Txthistorial.setText(null);
        Txtrepresentante.setText(null);
    }                                          

    private void ButLimpiarActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // PROGRAMA UN BOTON PARA LIMPIAR LA TABLA//
        DefaultTableModel modelo = (DefaultTableModel) Tbldatos.getModel();
        modelo.setRowCount(0);
        Txtnombre.setText(null);
        Txtnombrefut.setText(null);
        Txtapellidos.setText(null);
        Txtfechadenac.setText(null);
        Txtlugardenac.setText(null);
        Cmbprovincia.setSelectedItem(null);
        Spipie.setValue(null);
        Txtdemarcacion.setText(null);
        Txtsituacion.setText(null);
        Txtaltura.setText(null);
        Txtpeso.setText(null);
        Txtcantera.setText(null);
        Txtinternacional.setText(null);
        Txthistorial.setText(null);
        Txtrepresentante.setText(null);


    }                                          

    private void ButSalirActionPerformed(java.awt.event.ActionEvent evt) {                                         
        this.dispose();
    }                                        

